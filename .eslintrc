{
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true,
      "legacyDecorators": true,
    }
  },
  "extends": [
    "eslint:recommended",
  ],
  "rules": {
    "no-unused-vars": ["error", {
      "args": "after-used",
      "caughtErrors": "none",
      "ignoreRestSiblings": true
    }],
  }
}
